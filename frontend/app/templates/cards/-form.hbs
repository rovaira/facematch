<form {{action "save" model on="submit"}}>
  <p>
    <label>Name:
      {{input value=model.name}}
    </label>
  </p>
  <p>
    <label>Image URL:
      {{input value=model.image}}
    </label>
  </p>
  <input type="submit" value="Save"/>
</form>
<button {{action "cancel" model on="click"}}>Cancel</button>
